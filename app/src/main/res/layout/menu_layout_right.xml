<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动感音乐"
        android:textColor="#ffffff"
        android:id="@+id/textMusic"
        android:layout_above="@+id/textMap"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="52dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="搜索助手"
        android:textColor="#ffffff"
        android:id="@+id/text_Baidu_SS"
        android:layout_below="@+id/textMap"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="地图查看"
        android:id="@+id/textMap"
        android:textColor="#ffffff"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tab_icon_special"
        android:id="@+id/imageView2"
        android:layout_marginEnd="25dp"
        android:layout_alignBottom="@+id/textMusic"
        android:layout_toStartOf="@+id/textMusic" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tab_icon_special"
        android:id="@+id/imageView3"
        android:layout_alignBottom="@+id/textMap"
        android:layout_alignStart="@+id/imageView2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tab_icon_special"
        android:layout_alignBottom="@+id/text_Baidu_SS"
        android:layout_alignStart="@+id/imageView3" />



</RelativeLayout>